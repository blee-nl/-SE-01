{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathPrefix": "~",
            "rootPathSuffix": "./src"
          },
          {
            "rootPathPrefix": "#",
            "rootPathSuffix": "./src/assets"
          }
        ]
      }
    ],
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}